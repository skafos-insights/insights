
<ul>

<%= if @meeting.date do %>

  <li>
    <strong>Date:</strong>
    <%= @meeting.date %>
  </li>
<% end %>

<%= if @meeting.agenda_url do %>

  <li>
    <strong>Summary url:</strong>
    <%= @meeting.agenda_url %>
  </li>
<% end %>

<%= if @meeting.minutes_url do %>

  <li>
    <strong>Detail url:</strong>
    <%= @meeting.minutes_url %>
  </li>
<% end %>

<%= if @meeting.body do %>

  <li>
    <strong>Body:</strong>
    <p><%= raw(@html) %></p>
  </li>
<% end %>


</ul>

