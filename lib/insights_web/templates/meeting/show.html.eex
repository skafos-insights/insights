<h1>Show Meeting</h1>

<ul>

  <li>
    <strong>Date:</strong>
    <%= @meeting.date %>
  </li>

  <li>
    <strong>Summary url:</strong>
    <%= @meeting.summary_url %>
  </li>

  <li>
    <strong>Detail url:</strong>
    <%= @meeting.detail_url %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @meeting.body %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.meeting_path(@conn, :edit, @meeting) %></span>
<span><%= link "Back", to: Routes.meeting_path(@conn, :index) %></span>
